<!DOCTYPE html>
<html>
  <head>
    <title>Driver Scheduller</title>
    <link rel="stylesheet" href="/assets/css/styles.css" type="text/css" />
  </head>
  <body>

    <%- include('Partials/_nav') %>

    <div class="week">
      <div>
        <p>Hours</p>
      </div>
      <% for (let j = 0; j < week.length; j++) { %>
        <div>
          <p><%= week[j] %></p>
        </div> 
      <% } %>
    </div>

    <div id="bigboard">
      <div class="column">
        <p>8:00</p>
        <% for (let h = 1; h <= 10; h++) { %>
          <div>
          </div>
          <p><%= h + 8 %>:00</p> 
        <% } %>
      </div>

      <div id="board">
        <% for (let a = 0; a < 7; a++) { %>
          <div class="column">
          <% for (let b = 0; b < 10; b++) { %>
            <div class="divBoard">
              <p id="<%= a + '' + b %>"></p>
            </div>
          <% } %>
          </div>
        <% } %>
      </div>
    </div>

    <script type="text/javascript" src="../assets/scripts/app.js" data="<%= strData %>"></script>


  </body>
</html>